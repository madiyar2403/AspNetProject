@model IEnumerable<ProjectForDotNet.Models.Order>
@{
    ViewBag.Title = "OrderComplete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="container-fluid">
                <div class="row mb-1">
                    <div class="col-sm-6">
                        <h1>Order</h1>
                    </div>
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item">@Html.ActionLink("Home", "Index", "Home")</li>
                        </ol>
                    </div>
                </div>
            </div><!-- /.container-fluid -->
        </section>
        <div class="col-md-6">
            <div class="card card-default">
                <div class="card-header">
                    <h3 class="card-title">
                        <i class="fas fa-bullhorn"></i>
                        Order Complete successfully!
                    </h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <div class="callout callout-danger">
                        <h5>Order!</h5>

                        <p>
                            Dear, user, you will be contacted soon!
                        </p>
                    </div>
                    <div class="callout callout-info">
                        <h5>Delivery!</h5>

                        <p>Your order will come to you within 2 weeks!</p>
                    </div>
                    <div class="callout callout-warning">
                        <h5>Warning!</h5>

                        <p>For questions, go to the Contacts tab!</p>
                    </div>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
    </div>